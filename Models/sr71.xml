  <!-- Canopy -->
  <animation>
    <name>Canopy</name>
    <object-name>cockpit</object-name>
    <object-name>vitre0</object-name>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Canopy</object-name>
    <property>instrumentation/doors/crew/position-norm</property>
    <interpolation>
      <entry><ind> 0.00 </ind><dep>  0 </dep></entry>
      <entry><ind> 0.25 </ind><dep> 10 </dep></entry>
      <entry><ind> 0.50 </ind><dep> 15 </dep></entry>
      <entry><ind> 0.75 </ind><dep> 30 </dep></entry>
      <entry><ind> 1.00 </ind><dep> 38 </dep></entry>
    </interpolation>
    <center>
      <x-m> -9.616 </x-m>
      <y-m>  0.000 </y-m>
      <z-m>  0.169 </z-m>
    </center>
    <axis>
      <x>  0.00 </x>
      <y>  1.00 </y>
      <z>  0.00 </z>
    </axis>
  </animation>

  <!-- Parachute -->
  <animation>
    <type>rotate</type>
    <object-name>porteparaG</object-name>
    <property>instrumentation/doors/passenger/position-norm</property>
    <interpolation>
      <entry>
        <ind> 0.00 </ind>
        <dep> 0.00 </dep>
      </entry>
      <entry>
        <ind>  0.20 </ind>
        <dep> 90.00 </dep>
      </entry>
      <entry>
        <ind>  1.00 </ind>
        <dep> 95.00 </dep>
