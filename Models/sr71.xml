    </interpolation>
    <center>
      <x-m>  9.335 </x-m>
      <y-m> -0.324 </y-m>
      <z-m>  0.068 </z-m>
    </center>
    <axis>
      <x>  1.00 </x>
      <y>  0.00 </y>
      <z>  0.00 </z>
    </axis>
  </animation>

