    </axis>
  </animation>

  <!-- Sortie du parachute -->
  <animation>
    <type>select</type>
    <object-name>parachute</object-name>
    <condition>
      <greater-than>
        <property>instrumentation/doors/passenger/position-norm</property>
        <value>0.5</value>
      </greater-than>
    </condition>
  </animation>

