  <!-- Planches de bord -->
  <model>
    <path>Aircraft/Lockheed-SR71/Models/Panel/panel.xml</path>
    <offsets>
      <x-m> -10.706 </x-m>
      <y-m>   0.000 </y-m>
      <z-m>  -0.768 </z-m>
    </offsets>
  </model>

  <!-- Pilote invisible depuis le cockpit -->
  <animation>
    <type>select</type>
    <object-name>pilote</object-name>
    <condition>
      <greater-than>
        <property>sim/current-view/view-number</property>
        <value>0</value>
      </greater-than>
    </condition>
  </animation>

  <!-- Canopy -->
  <animation>
    <type>rotate</type>
    <object-name>cockpit</object-name>
    <object-name>vitre0</object-name>
    <property>instrumentation/doors/crew/position-norm</property>
    <interpolation>
      <entry>
        <ind> 0.00 </ind>
        <dep> 0.00 </dep>
      </entry>
      <entry>
        <ind>  0.25 </ind>
        <dep> 10.00 </dep>
      </entry>
      <entry>
        <ind>  0.50 </ind>
        <dep> 15.00 </dep>
      </entry>
      <entry>
        <ind>  0.75 </ind>
        <dep> 30.00 </dep>
      </entry>
      <entry>
        <ind>  1.00 </ind>
        <dep> 38.00 </dep>
      </entry>
    </interpolation>
    <center>
      <x-m> -9.616 </x-m>
      <y-m>  0.000 </y-m>
      <z-m>  0.169 </z-m>
    </center>
    <axis>
      <x>  0.00 </x>
      <y>  1.00 </y>
      <z>  0.00 </z>
    </axis>
  </animation>

