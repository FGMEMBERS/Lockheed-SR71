<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>vsi.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>visses</object-name>
    <object-name>face</object-name>
    <object-name>needle</object-name>

    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>glass.png</texture>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needle</object-name>
    <emission>
      <red>   0.8 </red>
      <green> 0.7 </green>
      <blue>  0.5 </blue>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>

  <animation>
    <name>needle</name>
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>velocities/vertical-speed-fps</property>
    <min>-100.0</min>
    <max>100.0</max>
    <scale>1.0</scale>
    <interpolation>
      <entry><ind> -100.00000000 </ind><dep> 106.0 </dep></entry>
      <entry><ind>  -83.33300000 </ind><dep> 126.0 </dep></entry>
      <entry><ind>  -66.66700000 </ind><dep> 146.0 </dep></entry>
      <entry><ind>  -50.00000000 </ind><dep> 166.0 </dep></entry>
      <entry><ind>  -33.33300000 </ind><dep> 191.0 </dep></entry>
      <entry><ind>  -16.66700000 </ind><dep> 223.5 </dep></entry>
      <entry><ind>    0.00000000 </ind><dep> 270.0 </dep></entry>
      <entry><ind>   16.66700000 </ind><dep> 313.5 </dep></entry>
      <entry><ind>   33.33300000 </ind><dep> 348.3 </dep></entry>
      <entry><ind>   40.92088198 </ind><dep> 360.0 </dep></entry>
      <entry><ind>   40.92088199 </ind><dep>   0.0 </dep></entry> 
      <entry><ind>   50.00000000 </ind><dep>  14.0 </dep></entry>
      <entry><ind>   66.66700000 </ind><dep>  34.0 </dep></entry>
      <entry><ind>   83.33300000 </ind><dep>  54.5 </dep></entry>
      <entry><ind>  100.00000000 </ind><dep>  74.0 </dep></entry>
    </interpolation>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center> 
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

</PropertyList>
